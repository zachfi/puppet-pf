{
  "name": "zleslie-pf",
  "version": "1.4.1",
  "author": "zleslie",
  "summary": "Manage PF with Puppet",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-pf.git",
  "project_page": "https://github.com/xaque208/puppet-pf",
  "issues_url": "https://github.com/xaque208/puppet-pf/issues",
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "10"
      ]
    },
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.1",
        "6.0",
        "5.9"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.1.0 < 5.0.0"
    }
  ],
  "description": "Manage the OpenBSD Packet Filtter with Puppet",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    },
    {
      "name": "dalen/puppetdbquery",
      "version_requirement": ">= 1.3.0"
    }
  ]
}
